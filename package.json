{
  "name": "vue-multi-tag",
  "version": "0.0.1",
  "author": "wbing",
  "description": "Vue3.4 + Pinia2.2 + TS5.4 + Vite5.4 + ElementPlus2.8 + VueSimpleUploader1.0.3 + PlusProComponents0.1.16 多页签脚手架，包含权限、UI组件等",
  "keywords": [
    "vue3",
    "multi-tag",
    "多页签脚手架"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "prepare": "husky"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@types/spark-md5": "^3.0.4",
    "axios": "^1.7.7",
    "element-plus": "^2.8.0",
    "lodash": "^4.17.21",
    "pinia": "^2.1.7",
    "plus-pro-components": "^0.1.16",
    "query-string": "^9.1.0",
    "spark-md5": "^3.0.2",
    "vue": "^3.5.6",
    "vue-router": "^4.3.3",
    "vue-simple-uploader": "^1.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@plus-pro-components/resolver": "^0.0.3",
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "husky": "^9.1.6",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.10",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.2.5",
    "sass": "^1.77.8",
    "typescript": "~5.4.0",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-icons": "^0.19.2",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.6",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-vue-devtools": "^7.3.1",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.21"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{scss,less,html}": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
